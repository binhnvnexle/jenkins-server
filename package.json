{
    "name": "nestjs-starter",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prisma:dev:deploy": "prisma migrate deploy",
        "db:dev:rm": "docker compose rm dev-db -s -f -v",
        "db:dev:up": "docker compose up dev-db -d",
        "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && sleep 2 && yarn prisma:dev:deploy",
        "redis:dev:rm": "docker compose rm dev-redis -s -f -v",
        "redis:dev:up": "docker compose up dev-redis -d",
        "redis:dev:restart": "yarn redis:dev:rm && yarn redis:dev:up",
        "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
        "prisma:open:studio:dev": "npx prisma studio",
        "prisma:open:studio:test": "dotenv -e .env.test npx prisma studio",
        "db:test:rm": "docker compose rm test-db -s -f -v",
        "db:test:up": "docker compose up test-db -d",
        "db:test:restart": "yarn db:test:rm && yarn db:test:up && sleep 2 && yarn prisma:test:deploy",
        "redis:test:rm": "docker compose rm test-redis -s -f -v",
        "redis:test:up": "docker compose up test-redis -d",
        "redis:test:restart": "yarn redis:test:rm && yarn redis:test:up",
        "seed": "ts-node prisma/seed.ts",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "pretest:e2e": "yarn redis:test:restart && yarn db:test:restart",
        "test:e2e": "dotenv -e .env.test -- jest --watch --no-cache --config ./test/jest-e2e.json",
        "api:dev:rm": "docker compose rm dev-api -fsv && docker compose rm dev-redis -fs && docker compose rm dev-db -fs",
        "api:dev:build": "docker compose up dev-api --build -V",
        "api:dev:up": "docker compose up dev-api -d",
        "api:dev:rebuild": "yarn api:dev:rm && yarn api:dev:build",
        "api:dev:restart": "yarn api:dev:rm && yarn api:dev:up"
    },
    "dependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/jwt": "^9.0.0",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/swagger": "^6.1.2",
        "@sentry/node": "^7.13.0",
        "argon2": "^0.29.1",
        "cache-manager": "^4.1.0",
        "cache-manager-redis-store": "^2.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "express-basic-auth": "^1.2.1",
        "nest-winston": "^1.7.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "sha1": "^1.1.1",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@prisma/client": "^4.3.0",
        "@types/cache-manager": "^4.0.2",
        "@types/express": "^4.17.13",
        "@types/jest": "28.1.4",
        "@types/node": "^16.0.0",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "dotenv-cli": "^6.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "28.1.2",
        "pactum": "^3.2.1",
        "prettier": "^2.3.2",
        "prisma": "^4.3.0",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "28.0.5",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.0.0",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "prisma": {
        "seed": "ts-node src/database/seed.ts",
        "schema": "src/database/schema.prisma"
    }
}
